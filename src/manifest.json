{
    "manifest_version": 2,
    "name": "Fair and Balanced",

    "description": "Rates malicious news sites on a 1-4 scale",

    "version": "1.0.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Fair and Balanced!"
    },


    "background": {
        "scripts": ["popup.js"]
    },


    "permissions": [
    "tabs", "webNavigation"
  ]





}